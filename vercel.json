{
  "version": 2,
  "builds": [
    {
      "src": "api/cursos.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/modulos.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/contenido.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/cursos",
      "dest": "api/cursos.js"
    },
    {
      "src": "/api/modulos",
      "dest": "api/modulos.js"
    },
    {
      "src": "/api/contenido",
      "dest": "api/contenido.js"
    }
  ]
}
